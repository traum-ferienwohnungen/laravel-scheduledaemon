{
    "name": "traum-ferienwohnungen/scheduledaemon",
    "description": "ScheduleDaemon for Laravel. You must ensure every schedule:run is shorter then 60 seconds otherwise events might be ignored.",
    "type": "library",
    "keywords": [
        "laravel"
    ],
    "require": {
        "illuminate/support": "^5.5",
        "illuminate/console": "^5.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Till Backhaus",
            "email": "backhaus@traum-ferienwohnungen.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "ScheduleDaemon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScheduleDaemon\\ServiceProvider"
            ]
        }
    }
}
